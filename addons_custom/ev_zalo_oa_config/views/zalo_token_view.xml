<?xml version="1.0"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="config_zalo_token_view_tree_inherit">
            <field name="name">config_zalo_token_view_tree_inherit</field>
            <field name="model">zalo.token</field>
            <field name="inherit_id" ref="ev_zalo_notification_service.zalo_token_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='app_id']" position="after">
                    <field name="oa_id"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="config_zalo_token_view_from_inherit">
            <field name="name">config_zalo_token_view_from_inherit</field>
            <field name="model">zalo.token</field>
            <field name="inherit_id" ref="ev_zalo_notification_service.zalo_token_view_from"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='app_id']" position="after">
                    <field name="oa_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>