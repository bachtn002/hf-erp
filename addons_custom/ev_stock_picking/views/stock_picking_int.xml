<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

	<record id="action_picking_tree_int" model="ir.actions.act_window">
        <field name="name">Picking Internal</field>
        <field name="res_model">stock.picking</field>
        <field name="view_mode">tree,kanban,form,calendar</field>
        <field name="domain">[('picking_type_id.code','=','internal')]</field>
        <field name="context">{'create': False, 'contact_display': 'partner_address', 'default_company_id': allowed_company_ids[0]}</field>
        <field name="search_view_id" ref="stock.view_picking_internal_search"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Picking Internal found. Let's create one!
            </p><p>
                Transfers allow you to move products from one location to another.
            </p>
        </field>
    </record>

	<menuitem id="menu_stock_picking_int" sequence="7" parent="stock.menu_stock_warehouse_mgmt" name="Stock Picking Internal"
	          groups="stock.group_stock_user" action="action_picking_tree_int"/>

</odoo>