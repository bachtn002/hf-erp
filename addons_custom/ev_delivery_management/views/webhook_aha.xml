<odoo>
    <data>
        <record id="webhook_aha_view_tree" model="ir.ui.view">
            <field name="name">Webhook Aha Tree</field>
            <field name="model">webhook.aha</field>
            <field name="arch" type="xml">
                <tree string="Webhook Aha Tree" create="0" delete="0">
                    <field name="data"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="webhook_aha__view_from" model="ir.ui.view">
            <field name="name">Webhook Aha Form</field>
            <field name="model">webhook.aha</field>
            <field name="arch" type="xml">
                <form string="Webhook Aha Form" create="0" delete="0" edit="0">
                    <header>
                        <button name="action_confirm" type="object" states="draft"
                                string="Confirm" class="oe_highlight" invisible="1"/>
                        <field name="state" widget="statusbar"
                               statusbar_visible="draft,queue,done"
                               clickable="0"/>
                    </header>
                    <sheet>
                        <group>
                            <field name="data" style="word-break: break-word" readonly="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="webhook_aha_action" model="ir.actions.act_window">
            <field name="name">Webhook Aha</field>
            <field name="res_model">webhook.aha</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="webhook_aha_menu" name="Webhook Aha" action="webhook_aha_action"
                  parent="ev_delivery_management.delivery_menu_root"
                  groups="base.group_system"
                  sequence="4"/>
    </data>
</odoo>