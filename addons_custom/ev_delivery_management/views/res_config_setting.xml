<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="res_config_settings_call_ship_view_form_inherit">
            <field name="name">res_config_settings_call_ship_view_form_inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='warning_info']" position="after">
                    <div class="col-12 col-lg-6 o_setting_box" id="stock_call_ship">
                        <div class="o_setting_left_pane">
                            <field name="x_call_ship"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="x_call_ship"/>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>