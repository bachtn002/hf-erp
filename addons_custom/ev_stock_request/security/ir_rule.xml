<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">
        <!--    stock_request-->
        <record model="ir.rule" id="stock_request_multi_company_rule">
            <field name="name">Stock Request Multi Company Rule</field>
            <field name="model_id" ref="ev_stock_request.model_stock_request"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>

        <record id="group_stock_coordinator" model="res.groups">
            <field name="name">Coordinator</field>
            <field name="category_id" ref="base.module_category_inventory_inventory"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>
	</data>
</odoo>