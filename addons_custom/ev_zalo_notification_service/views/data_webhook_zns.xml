<odoo>
    <data>
        <record id="data_webhook_zns_view_tree" model="ir.ui.view">
            <field name="name">Data Webhook Zns Tree</field>
            <field name="model">data.webhook.zns</field>
            <field name="arch" type="xml">
                <tree string="Data Webhook Zns Tree" create="0" delete="0">
                    <field name="data"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="data_webhook_zns__view_from" model="ir.ui.view">
            <field name="name">Data Webhook Zns Form</field>
            <field name="model">data.webhook.zns</field>
            <field name="arch" type="xml">
                <form string="Data Webhook Zns Form" create="0" delete="0" edit="0">
                    <header>
                        <button name="action_confirm" type="object" states="draft"
                                string="Confirm" class="oe_highlight"/>
                        <field name="state" widget="statusbar"
                               statusbar_visible="draft,queue,done"
                               clickable="0"/>
                    </header>
                    <sheet>
                        <group>
                            <field name="data" style="word-break: break-word" readonly="1"/>
                            <field name="message" style="word-break: break-word" readonly="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="data_webhook_zns_action" model="ir.actions.act_window">
            <field name="name">Data Webhook Zns</field>
            <field name="res_model">data.webhook.zns</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>