<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <!-- Job for check Expired Certificated-->
        <record forcecreate="True" id="ir_cron_expired_certificated" model="ir.cron">
            <field name="name">Survey User Input: Job Check Expired Certificated</field>
            <field name="model_id" ref="model_survey_user_input"/>
            <field name="state">code</field>
            <field name="code">model._state_expired_certificated()</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 01:00:00')" />
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
    </data>
</odoo>