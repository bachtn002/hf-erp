<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="postgresql_materialized_init_cron" model="ir.cron">
            <field name="name">PostgreSQL Materialized: Init</field>
            <field name="model_id" ref="model_postgresql_materialized"/>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now().replace(day=1, hour=1, minute=0)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="state">code</field>
            <field name="code">model.action_cron_previous_month_materialized()</field>
        </record>
    
    </data>

</odoo>
