<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="consumption_forecast_form_inherit_view" model="ir.ui.view">
            <field name="name">consumption_forecast_form</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='out_minus']" position="after">
					<field name="consumption_forecast"/>
				</xpath>
            </field>
        </record>
    </data>
</odoo>
