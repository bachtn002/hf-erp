<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="api_users_job" model="ir.cron">
        <field name="name">Base: Api users data</field>
        <field name="model_id" ref="model_log_user_api"/>
        <field name="state">code</field>
        <field name="code">model._call_api_user()</field>
        <field name="nextcall" eval="datetime.now(pytz.timezone('UTC')).strftime('%Y-%m-%d 21:00:00')"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
    </record>
</odoo>