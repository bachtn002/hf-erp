<odoo>
    <data>
        <record id="stock_in_out_report_view_tree" model="ir.ui.view">
            <field name="name">Stock In Out Report View Tree</field>
            <field name="model">stock.in.out.report</field>
            <field name="arch" type="xml">
                <tree string="Stock In Out Report View Tree" create="0" delete="0">
                    <field name="location"/>
                    <field name="default_code"/>
                    <field name="product"/>
                    <field name="uom"/>
                    <field name="qty_begin" sum="Qty Begin Total"/>
                    <field name="qty_in" sum="Qty In Total"/>
                    <field name="qty_out" sum="Qty Out Total"/>
                    <field name="qty_end" sum="Qty End Total"/>
                </tree>
            </field>
        </record>
        <record id="stock_in_out_report_view_form" model="ir.ui.view">
            <field name="name">Stock In Out Report View Form</field>
            <field name="model">stock.in.out.report</field>
            <field name="arch" type="xml">
                <form string="Stock In Out Report View Form" create="0" edit="0" delete="0" duplicate="0">
                    <sheet>
                        <group>
                            <group>
                                <field name="location" readonly="1"/>
                                <field name="default_code" readonly="1"/>
                                <field name="product" readonly="1"/>
                                <field name="uom" readonly="1"/>
                            </group>
                            <group>
                                <field name="qty_begin" readonly="1"/>
                                <field name="qty_in" readonly="1"/>
                                <field name="qty_out" readonly="1"/>
                                <field name="qty_end" readonly="1"/>
                            </group>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>
        <record id="stock_in_out_report_view_search" model="ir.ui.view">
            <field name="name">Stock In Out Report Search</field>
            <field name="model">stock.in.out.report</field>
            <field name="arch" type="xml">
                <search string="Search">
                    <field name="location" string="Location Warehouse" filter_domain="[('location','ilike',self)]"/>
                    <field name="default_code" string="Default Code" filter_domain="[('default_code','ilike',self)]"/>
                    <field name="product" string="Product" filter_domain="[('product','ilike',self)]"/>
                </search>
            </field>
        </record>

        <record id="stock_in_out_report_action" model="ir.actions.act_window">
            <field name="name">Stock In Out Report Action</field>
            <field name="res_model">stock.in.out.report</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="stock_in_out_report_view_search"/>
        </record>
    </data>
</odoo>