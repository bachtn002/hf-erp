<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="product_template_print_stamp_form_view">
            <field name="name">product_template_print_stamp_form_view</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='variants']" position="after">
                    <page string="Stamp Product">
                        <group>
                            <group>
                                <field name="x_origin"/>
                                <field name="x_print_stamp"/>
                                <field name="x_empty_date"/>
                                <field name="x_ingredient"/>
                            </group>
                            <group>
                                <field name="x_preserve"/>
                                <field name="x_expiry"/>
                                <field name="x_packers"/>
                                <field name="x_supplier_address"/>
                            </group>
                        </group>
                        <group>
                            <field name="x_use_manual"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
